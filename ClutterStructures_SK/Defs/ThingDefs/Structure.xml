<?xml version="1.0" encoding="utf-8"?>
<Buildings>
    <ThingDef Name="ClutterStructureBuildingBase" Abstract="True">
        <category>Building</category>
        <thingClass>Building</thingClass>
        <soundImpactDefault>BulletImpactMetal</soundImpactDefault>
        <selectable>true</selectable>
        <drawerType>MapMeshAndRealTime</drawerType>
        <terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
        <repairEffect>Repair</repairEffect>
        <leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
        <filthLeaving>BuildingRubble</filthLeaving>
		<statBases>
			<SellPriceFactor>0.70</SellPriceFactor>
		</statBases>
    </ThingDef>
    <!-- Normal Fence -->
    <ThingDef ParentName="ClutterStructureBuildingBase">
        <defName>NormalFence</defName>
        <label>Small Fence</label>
        <thingClass>Building</thingClass>
        <category>Building</category>
        <graphicData>
            <linkType>Basic</linkType>
            <texPath>Clutter/Structure/Fence</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <shaderType>CutoutComplex</shaderType>
            <linkFlags>
                <li>Custom5</li>
            </linkFlags>
        </graphicData>
        <uiIconPath>Clutter/Structure/FenceTestico</uiIconPath>
        <AltitudeLayer>Building</AltitudeLayer>
        <pathCost>75</pathCost>
        <passability>PassThroughOnly</passability>
        <statBases>
            <MaxHitPoints>250</MaxHitPoints>
            <WorkToBuild>100</WorkToBuild>
            <Flammability>0.0</Flammability>
        </statBases>
        <fillPercent>0.3</fillPercent>
        <Description>To stop those pesky creatures eating from your storage.</Description>
        <placingDraggableDimensions>1</placingDraggableDimensions>
        <costList>
            <SteelBar>5</SteelBar>
        </costList>
        <costStuffCount>5</costStuffCount>
        <holdsRoof>false</holdsRoof>
        <selectable>true</selectable>
        <DesignationCategory>Structure</DesignationCategory>
        <researchPrerequisites>
            <li>SK_ConstructionI</li>
        </researchPrerequisites>
    </ThingDef>
	<!-- Concrete Fence -->
    <ThingDef ParentName="ClutterStructureBuildingBase">
        <defName>ConcreteFence</defName>
        <label>Concrete Fence</label>
        <thingClass>Building</thingClass>
        <category>Building</category>
        <graphicData>
            <linkType>Basic</linkType>
            <texPath>Clutter/Structure/ConcreteFence</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <shaderType>CutoutComplex</shaderType>
            <linkFlags>
                <li>Custom6</li>
            </linkFlags>
        </graphicData>
        <Description>High fence</Description>
        <uiIconPath>Clutter/Structure/ConcreteFenceIco</uiIconPath>
        <statBases>
            <MaxHitPoints>350</MaxHitPoints>
            <WorkToBuild>200</WorkToBuild>
            <Flammability>0.0</Flammability>
        </statBases>
        <costList>
            <SteelBar>5</SteelBar>
            <ConcreteResource>5</ConcreteResource>
        </costList>
        <costStuffCount>10</costStuffCount>
        <leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
        <altitudeLayer>Building</altitudeLayer>
        <passability>Impassable</passability>
        <fillPercent>0.69</fillPercent>
        <coversFloor>true</coversFloor>
        <placingDraggableDimensions>1</placingDraggableDimensions>
        <tickerType>Never</tickerType>
        <rotatable>false</rotatable>
        <selectable>true</selectable>
        <neverMultiSelect>true</neverMultiSelect>
        <terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
        <holdsRoof>false</holdsRoof>
        <designationCategory>Structure</designationCategory>
        <building>
            <isInert>true</isInert>
            <ignoreNeedsPower>true</ignoreNeedsPower>
        </building>
        <researchPrerequisites>
			<li>SK_ConstructionI</li>
        </researchPrerequisites>
    </ThingDef>
	<!-- Load-Bearing Wall -->
    <ThingDef Class="Clutter_Structure.ClutterStructureThingDefs" ParentName="ClutterStructureBuildingBase">
        <defName>ClutterUtilityWall</defName>
        <label>Load-Bearing Wall</label>
        <thingClass>Clutter_Structure.Wall_Multi</thingClass>
        <category>Building</category>
        <Description>Load-Bearing Walls. Strong enough to withstand any structural damage, its modular, you can install lights or windows.</Description>
        <graphicData>
			<texPath>Clutter/Structure/UWall</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <linkType>CornerFiller</linkType>
            <linkFlags>
                <li>Wall</li>
				<li>Rock</li>
            </linkFlags>
            <damageData>
                <cornerTL>Damage/Corner</cornerTL>
                <cornerTR>Damage/Corner</cornerTR>
                <cornerBL>Damage/Corner</cornerBL>
                <cornerBR>Damage/Corner</cornerBR>
                <edgeTop>Damage/Edge</edgeTop>
                <edgeBot>Damage/Edge</edgeBot>
                <edgeLeft>Damage/Edge</edgeLeft>
                <edgeRight>Damage/Edge</edgeRight>
            </damageData>
        </graphicData>
        <blueprintGraphicData>
            <texPath>Things/Building/Linked/Wall_Blueprint_Atlas</texPath>
        </blueprintGraphicData>
		<uiIconPath>Clutter/Structure/UWallIco</uiIconPath>
        <statBases>
            <MaxHitPoints>600</MaxHitPoints>
            <WorkToBuild>500</WorkToBuild>
            <Flammability>0.0</Flammability>
            <Beauty>7</Beauty>
        </statBases>
        <stuffCategories>
            <li>Metallic</li>
        </stuffCategories>
        <costStuffCount>12</costStuffCount>
        <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
        <altitudeLayer>Building</altitudeLayer>
        <passability>Impassable</passability>
        <blockWind>true</blockWind>
        <castEdgeShadows>true</castEdgeShadows>
        <fillPercent>1</fillPercent>
        <coversFloor>true</coversFloor>
        <placingDraggableDimensions>1</placingDraggableDimensions>
        <tickerType>Never</tickerType>
        <rotatable>false</rotatable>
        <selectable>true</selectable>
        <neverMultiSelect>true</neverMultiSelect>
        <terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
        <holdsRoof>true</holdsRoof>
        <designationCategory>Structure</designationCategory>
        <staticSunShadowHeight>1.0</staticSunShadowHeight>
        <blockLight>true</blockLight>
        <building>
            <isInert>true</isInert>
            <ignoreNeedsPower>true</ignoreNeedsPower>
        </building>
        <researchPrerequisites>
            <li>SK_ConstructionII</li>
        </researchPrerequisites>
		<SecondaryMaterialPath>Clutter/Structure/UWallA</SecondaryMaterialPath>
        <ThirdMaterialPath>Clutter/Structure/UWallB</ThirdMaterialPath>
    </ThingDef>
	<!-- Load-Bearing Wall Lights -->
    <ThingDef Class="Clutter_Structure.ClutterStructureThingDefs" ParentName="ClutterStructureBuildingBase">
        <defName>ClutterUWallLight</defName>
        <label>Load-Bearing Wall Light</label>
        <thingClass>Clutter_Structure.Wall_Light</thingClass>
        <category>Building</category>
        <Description>Load-Bearing Walls. Strong enough to withstand any structural damage, its modular, you can install lights or windows.</Description>
        <graphicData>
			<texPath>Clutter/Structure/UWallYellow</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <linkType>CornerFiller</linkType>
            <linkFlags>
                <li>Wall</li>
				<li>Rock</li>
            </linkFlags>
            <damageData>
                <cornerTL>Damage/Corner</cornerTL>
                <cornerTR>Damage/Corner</cornerTR>
                <cornerBL>Damage/Corner</cornerBL>
                <cornerBR>Damage/Corner</cornerBR>
                <edgeTop>Damage/Edge</edgeTop>
                <edgeBot>Damage/Edge</edgeBot>
                <edgeLeft>Damage/Edge</edgeLeft>
                <edgeRight>Damage/Edge</edgeRight>
            </damageData>
        </graphicData>
        <blueprintGraphicData>
            <texPath>Things/Building/Linked/Wall_Blueprint_Atlas</texPath>
        </blueprintGraphicData>
		<uiIconPath>Clutter/Structure/UWallIco</uiIconPath>
        <statBases>
            <MaxHitPoints>600</MaxHitPoints>
            <WorkToBuild>500</WorkToBuild>
            <Flammability>0.0</Flammability>
            <Beauty>7</Beauty>
        </statBases>
		<stuffCategories>
            <li>Metallic</li>
        </stuffCategories>
        <costStuffCount>1</costStuffCount>
        <CostList>
            <Wire>2</Wire>
            <Glass>5</Glass>
        </CostList>
        <AltitudeLayer>Building</AltitudeLayer>
        <Passability>Impassable</Passability>
        <castEdgeShadows>true</castEdgeShadows>
        <fillPercent>1</fillPercent>
        <coversFloor>true</coversFloor>
        <rotatable>false</rotatable>
        <selectable>true</selectable>
        <neverMultiSelect>true</neverMultiSelect>
        <terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
        <holdsRoof>true</holdsRoof>
